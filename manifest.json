{
  "manifest_version": 3,
  "name": "Bookmark Folder",
  "version": "1.0",
  "description": "A simple Chrome extension to manage a list of favorite links.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}